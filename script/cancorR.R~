#f <- matrix(c(scale(x1), scale(x2)),,2)
#g <- matrix(c(scale(x3), scale(x4)),,2)
#print(f)
#print(g)

f <- matrix(c(x1, x2),,2)
g <- matrix(c(x3, x4),,2)

can <- cancor(f,g)
print(can)

cat("t2/t1")
print(t2/t1)
cat("z2/z1")
print(z$vectors[2,1]/z$vectors[1,1])
cat("c2/c1")
print(can$xcoef[2,1]/can$xcoef[1,1])
